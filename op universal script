 local Button = MM2Tab:CreateButton({
     Name = "Bad ESP",
     Callback = function()
          -- Roblox Murder Mystery 2 ESP Script
  
 -- Function to create ESP box
 local function createESP(character, color)
     local espBox = Instance.new("BoxHandleAdornment")
     espBox.Size = character.HumanoidRootPart.Size + Vector3.new(0, 2, 0) -- Extend ESP to cover whole body
     espBox.Adornee = character.HumanoidRootPart
     espBox.AlwaysOnTop = true
     espBox.ZIndex = 5
     espBox.Color3 = color
     espBox.Transparency = 0.3
     espBox.Parent = character
 end
  
 -- Function to check role and create ESP accordingly
 local function checkRoleAndESP(player)
     local character = player.Character
     if not character then return end
  
     local role = player.Backpack:FindFirstChild("Knife") and "Murderer"
         or player.Backpack:FindFirstChild("Gun") and "Sheriff"
         or "Innocent"
     local espColor = nil
  
     -- Assign color based on role
     if role == "Murderer" then
         espColor = Color3.fromRGB(255, 0, 0) -- Bright Red for Murderer
     elseif role == "Sheriff" then
         espColor = Color3.fromRGB(0, 0, 255) -- Bright Blue for Sheriff
     elseif role == "Innocent" then
         espColor = Color3.fromRGB(0, 255, 0) -- Bright Green for Innocent
     end
  
     -- Create ESP box
     if espColor then
         createESP(character, espColor)
     end
 end
  
 -- Loop through players and apply ESP
 for _, player in pairs(game:GetService("Players"):GetPlayers()) do
     checkRoleAndESP(player)
 end
  
 -- Listen for new players and apply ESP
 game:GetService("Players").PlayerAdded:Connect(function(player)
     player.CharacterAdded:Connect(function(character)
         wait(1) -- Wait for character to fully load
         checkRoleAndESP(player)
     end)
 end)
  
     end,
  })
  
